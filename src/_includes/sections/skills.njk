{% if data.skills %}
<section class="section">
  <div class="section-header">
    <h2>{{ data.skills.title }}</h2>
  </div>
  <div class="chips">
    {% for skill in data.skills.toolset %}
    <div class="chip">
      <span>{{ skill.name }}</span>
      <small>{{ skill.level }}</small>
    </div>
    {% endfor %}
  </div>
</section>
{% endif %}
